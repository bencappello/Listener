<h1><%= band.get('name') %></h1>

<h5>Genres</h5>
<ul class='tags'>
  <% band.tags().forEach(function (tag) { %>
    <li><%= tag.get('name') %></li>
  <% }) %>
</ul>

<h5>Songs</h5>
<ul class='songs'>
  <% band.songs().forEach(function (song) { %>
    <li><a data-id='<%= song.id %>' href="#"><%= song.get('name') %></a></li>
  <% }) %>
</ul>


<h4>Comments</h4>
<% band.comments().forEach(function (comment) { %>
  <p><%= comment.get('author_name') %>: <%= comment.get('body') %></p>
  <button class="edit-button"><a class='edit-link' href="#">Edit</a></button>
  <button class="delete-button"><a class='delete-link' href="#">Delete</a></button>
<% }) %>
